<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head') %>
</head>

<body>
    <%- include('partials/header') %>
    <table id="matches-table">
        <tr>
            <th>Match</th>
            <th>Home Team</th>
            <th>Away Team</th>
            <th>Date</th>
            <th>Time</th>
        </tr>
        <% matchesTable.forEach(match => { %>
            <tr>
            <td><%= match.matchname %></td>
            <td><%= match.hometeam %></td>
            <td><%= match.awayteam %></td>
            <td><%= match.matchdate %></td>
            <td><%= match.matchtime %></td>
        </tr>
        <% }); %>
    </table>
    <%- include('partials/head') %>
</body>
</html>
